<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <TextView
            android:id="@+id/tv_url"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="200dp"
            android:gravity="center"
            android:background="#99000000"
            android:textColor="#fff"
            android:text="https://cn.bing.com"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

    <Button
            android:id="@+id/btn_setUrl"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="设置Url"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_url"/>

    <Button
            android:id="@+id/btn_webview"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="系统内核加载"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_setUrl"/>

    <Button
            android:id="@+id/btn_x5_webview"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:text="腾讯X5内核加载"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_webview"/>


</androidx.constraintlayout.widget.ConstraintLayout>